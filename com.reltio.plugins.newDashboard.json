{
    "id": "com.reltio.plugins.newDashboard",
    "name": "New Dashboard Perspective",
    "version": "1.0.0",
    "provider-name": "Reltio.com",
    "requirements": [
        "com.reltio.plugins.entity"
    ],
    "extensions": [
        {
            "point": "com.reltio.plugins.ui.perspective",
            "id": "com.reltio.plugins.newDashboard.NewDashboardPerspective",
            "class": "com.reltio.plugins.ui.Perspective",
            "layout": {
                "layout": "scroll",
                "items": [
                    {
                        "view": "com.reltio.plugins.newDashboard.NewDashboardView"
                    }
                ]
            },
            "react-perspective": "Dashboard"
        },
        {
            "point": "com.reltio.plugins.ui.view",
            "id": "com.reltio.plugins.newDashboard.NewDashboardView",
            "class": "com.reltio.plugins.react.ReactDashboardView",
            "react-class": "com.reltio.plugins.react.ReactDashboardView",
            "views": [
                {
                    "id": "com.reltio.plugins.dashboard.ProfileStatsView",
                    "component": "ProfileStatsFacet",
                    "title": "Profile Stats"
                },
                {
                    "id": "com.reltio.plugins.dashboard.OrgsUSAMapView",
                    "chartType": "map",
                    "component": "DashboardFacet",
                    "title": "USA Individual View",
                    "geoUrl": "https://reltio-ui-files.s3.amazonaws.com/map_resources/new_dashboard/usa_prefix_latlng.topo.json",
                    "projection": "geoMercator",
                    "attributeUri": "configuration/entityTypes/Individual/attributes/Addresses/attributes/StateProvince"
                },
                {
                    "id": "com.reltio.plugins.dashboard.OrgsAddressDataQualityView",
                    "chartType": "bubble",
                    "component": "DashboardFacet",
                    "title": "Individual Address Data Quality",
                    "attributeUri": "configuration/entityTypes/Individual/attributes/Addresses/attributes/VerificationStatus"
                },
                {
                    "id": "com.reltio.plugins.dashboard.OrgsWithMissingEmailView",
                    "filters": [],
                    "chartType": "map",
                    "component": "DashboardFacet",
                    "title": "USA Account View",
                    "geoUrl": "https://reltio-ui-files.s3.amazonaws.com/map_resources/new_dashboard/usa_prefix_latlng.topo.json",
                    "projection": "geoMercator",
                    "attributeUri": "configuration/entityTypes/Account/attributes/Addresses/attributes/StateProvince"
                },
                {
                    "id": "com.reltio.plugins.dashboard.ContactsAddressDataQualityView",
                    "chartType": "bubble",
                    "component": "DashboardFacet",
                    "title": "Organization Address Data Quality",
                    "attributeUri": "configuration/entityTypes/Organization/attributes/Addresses/attributes/VerificationStatus"
                },
                {
                    "id": "com.reltio.plugins.notifications.Inbox",
                    "component": "NotificationInboxFacet",
                    "title": "Notifications"
                },
                {
                    "id": "com.reltio.plugins.activities.ActivitiesView",
                    "component": "ActivitiesViewFacet",
                    "title": "Activity log"
                },
                {
                    "id": "e0a5-44f4-8231c48d",
                    "component": "DashboardFacet",
                    "title": "USA Organization View",
                    "chartType": "map",
                    "attributeUri": "configuration/entityTypes/Organization/attributes/Addresses/attributes/StateProvince",
                    "geoUrl": "https://reltio-ui-files.s3.amazonaws.com/map_resources/new_dashboard/usa_prefix_latlng.topo.json",
                    "projection": "geoMercator"
                },
                {
                    "id": "008f-28a2-d0da89ee",
                    "component": "DashboardFacet",
                    "title": "Account Address Data Quality",
                    "chartType": "bubble",
                    "attributeUri": "configuration/entityTypes/Account/attributes/Addresses/attributes/VerificationStatus"
                },
                {
                    "id": "f38b-9f36-a6ce2ee7",
                    "component": "FilteredSavedSearchesFacet",
                    "title": "Saved searches",
                    "options": {
                        "favoriteOnly": false,
                        "countResults": true
                    }
                },
                {
                    "id": "b357-fe04-eb0cb656",
                    "component": "DashboardFacet",
                    "title": "City Status",
                    "chartType": "bubble",
                    "attributeUri": "configuration/entityTypes/Individual/attributes/Addresses/attributes/City"
                }
            ],
            "layout": [
                {
                    "x": 0,
                    "y": 0,
                    "width": 3,
                    "height": 10,
                    "id": "com.reltio.plugins.dashboard.ProfileStatsView",
                    "isStatic": false
                },
                {
                    "x": 0,
                    "y": 10,
                    "width": 1,
                    "height": 10,
                    "id": "com.reltio.plugins.dashboard.OrgsUSAMapView",
                    "isStatic": false
                },
                {
                    "x": 0,
                    "y": 20,
                    "width": 1,
                    "height": 10,
                    "id": "com.reltio.plugins.dashboard.OrgsAddressDataQualityView",
                    "isStatic": false
                },
                {
                    "x": 2,
                    "y": 10,
                    "width": 1,
                    "height": 10,
                    "id": "com.reltio.plugins.dashboard.OrgsWithMissingEmailView",
                    "isStatic": false
                },
                {
                    "x": 1,
                    "y": 20,
                    "width": 1,
                    "height": 10,
                    "id": "com.reltio.plugins.dashboard.ContactsAddressDataQualityView",
                    "isStatic": false
                },
                {
                    "x": 3,
                    "y": 0,
                    "width": 1,
                    "height": 10,
                    "id": "com.reltio.plugins.notifications.Inbox",
                    "isStatic": false
                },
                {
                    "x": 3,
                    "y": 10,
                    "width": 1,
                    "height": 10,
                    "id": "com.reltio.plugins.activities.ActivitiesView",
                    "isStatic": false
                },
                {
                    "x": 1,
                    "y": 10,
                    "width": 1,
                    "height": 10,
                    "minHeight": 10,
                    "id": "e0a5-44f4-8231c48d",
                    "isStatic": false
                },
                {
                    "x": 2,
                    "y": 20,
                    "width": 1,
                    "height": 10,
                    "minHeight": 10,
                    "id": "008f-28a2-d0da89ee",
                    "isStatic": false
                },
                {
                    "x": 3,
                    "y": 20,
                    "width": 1,
                    "height": 10,
                    "minHeight": 10,
                    "id": "f38b-9f36-a6ce2ee7",
                    "isStatic": false
                },
                {
                    "x": 0,
                    "y": 30,
                    "width": 1,
                    "height": 10,
                    "minHeight": 10,
                    "id": "b357-fe04-eb0cb656",
                    "isStatic": false
                }
            ]
        }
    ]
}